{
    "$id": "https://example.com/person.schema.json",
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "NumberExample",
    "type": "object",
    "properties": {
        "weight": {
            "type": "number",
            "minimum": 0,
            "maximum": 5
        },
        "height": {
            "type": "number",
            "exclusiveMinimum": 9,
            "exclusiveMaximum": 21
        },
        "age" : {
            "type" : "number",
            "multipleOf": 5
        }
    }
}