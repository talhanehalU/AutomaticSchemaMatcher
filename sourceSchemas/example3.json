{
    "$id": "https://example.com/person.schema.json",
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "StringExample",
    "type": "object",
    "properties": {
        "employeeName": {
            "type": "string",
            "minLength": 6,
            "maxLength": 9
        },
        "employeeEmail": {
            "type": "string",
            "pattern": "[a-z]+@example.com"
        }
    }
}